echo "~/kafka/bin/kafka-console-consumer.sh --zookeeper zookeeper-04.prod.visualdna.com $@" ./kafka8 && chmod a+x ./kafka8

#test datasync 
cat src/main/resources/datasync.json | jq -r 'select(.partnerUserId!=null and .userUid!=null) | .userUid,.timestamp,.idSpace,.partnerUserId'

#stream of syncs
~/kafka/bin/kafka8 --topic datasync \
| jq -r 'select(.partnerUserId!=null and .userUid!=null) | .userUid,.timestamp,.idSpace,.partnerUserId' \
| java -jar target/aim-node.jar --port 4000 --schema "example=user_uid(UUID:BYTEARRAY[16]),timestamp(LONG),column(STRING),value(STRING)"

#test pageviews
cat src/main/resources/pageviews.json | jq -r 'select(.userUid!=null) | .userUid,.timestamp,.type,.url'

#stream of pageviews
~/kafka/bin/kafka8 --topic pageviews \
| jq -r 'select(.userUid!=null and .type!=null) | .userUid,.timestamp,.type,.url' \
| java -jar target/aim-node.jar--port 4001 --schema "example=user_uid(UUID:BYTEARRAY[16]),timestamp(LONG),column(STRING),value(STRING)"

